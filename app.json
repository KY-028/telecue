{
  "expo": {
    "name": "TeleCue",
    "slug": "telecue",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/telecue_logo.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/telecue_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ky028sorganization.telecue"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/telecue_logo.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS",
        "android.permission.WRITE_SETTINGS"
      ],
      "package": "com.ky028sorganization.telecue"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-sqlite",
      [
        "react-native-vision-camera",
        {
          "cameraPermissionText": "$(PRODUCT_NAME) needs access to your Camera.",
          "microphonePermissionText": "$(PRODUCT_NAME) needs access to your Microphone.",
          "enableMicrophonePermission": true
        }
      ],
      "expo-screen-orientation",
      "expo-brightness",
      "expo-localization",
      "expo-secure-store"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "e16e6cca-0db0-401b-9cf4-396b4c582a3a"
      }
    },
    "owner": "ky028s-organization"
  }
}
